<h1>{{title}}</h1>
<form action="/api/members" method="POST" class="mb-4">
    <div class="form-group">
        <label for="name">Name</label>
        <input for="text" name="name" class="form-control"></input>
    </div>
    <div class="form-group">
        <label for="email">Email</label>
        <input for="email" name="email" class="form-control"></input>
    </div>
    <input type="submit" value="Add Member" class="btn btn-primary btn-clock">
</form>
<h4>Members</h4>
<ul class="list-group">
    {{#each members}}
        <li class="list-group-item">{{this.name}} : {{this.email}}</li>
    {{/each}}
</ul>
{{!-- {{}} - variable --}}